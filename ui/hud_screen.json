{
    "namespace": "hud",
    "vignette_renderer": {
        "renderer": "flying_item_renderer"
    },
    "hud_title_text": {
        "offset": [
            "0%", // x
            "-30%" // y
        ]
    },

    "hud_title_text/subtitle_frame/subtitle_background": {
        "alpha": 0
    },
    "hud_title_text/title_frame/title_background": {
        "alpha": 0
    },

    "hud_title_text/subtitle_frame/subtitle": {
        "type": "label",
        "shadow": true,
        "font_scale_factor": 0.6,
        "offset": [
            0, // x
            -40 // y
        ]
    },
    "hud_title_text/title_frame/title": {
        "type": "label",
        "shadow": true,
        "font_scale_factor": 0.6
    }
}